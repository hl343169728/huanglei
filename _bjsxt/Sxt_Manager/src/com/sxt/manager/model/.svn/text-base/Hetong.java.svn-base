package com.sxt.manager.model;

/**
 * 先就业后付款合同
 * @author ThinkPad
 *
 */
public class Hetong {
	private int id;
	private int studentId;
	private int userId; //录入者Id
	private double huankuanTotal;  //还款总额
	private String descrpt; 
	private String state="未还款";   //未还款，正在还款，还款完毕

	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getStudentId() {
		return studentId;
	}

	public void setStudentId(int studentId) {
		this.studentId = studentId;
	}

	public int getUserId() {
		return userId;
	}

	public void setUserId(int userId) {
		this.userId = userId;
	}

	public double getHuankuanTotal() {
		return huankuanTotal;
	}

	public void setHuankuanTotal(double huankuanTotal) {
		this.huankuanTotal = huankuanTotal;
	}


	public String getDescrpt() {
		return descrpt;
	}

	public void setDescrpt(String descrpt) {
		this.descrpt = descrpt;
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}
	
	
	
	
}
