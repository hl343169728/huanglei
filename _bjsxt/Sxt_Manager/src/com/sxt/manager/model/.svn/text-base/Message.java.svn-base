package com.sxt.manager.model;

import java.util.Date;

/**
 * 站内信
 * @author ThinkPad
 *
 */
public class Message {
	private int id;
	private String title;
	private String content;
	private String createTime; 					//创建时间
	private String sendTime;  					//定时发送时间
	private int senderId ;  					//发件人的id					userId
	private String  sendboxState ="有效";  		//默认值：有效。  可选值有：有效，删除 
	private String UUID;						//用于标识这个记录是唯一的
	private int realDelete ;					//真正物理删除标识	 默认为0  状态为 1 时表示物理删除
	
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getCreateTime() {
		return createTime;
	}

	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}

	public String getSendTime() {
		return sendTime;
	}

	public void setSendTime(String sendTime) {
		this.sendTime = sendTime;
	}

	public int getSenderId() {
		return senderId;
	}

	public void setSenderId(int senderId) {
		this.senderId = senderId;
	}

	public String getSendboxState() {
		return sendboxState;
	}

	public void setSendboxState(String sendboxState) {
		this.sendboxState = sendboxState;
	}

	public String getUUID() {
		return UUID;
	}

	public void setUUID(String uUID) {
		UUID = uUID;
	}

	public int getRealDelete() {
		return realDelete;
	}

	public void setRealDelete(int realDelete) {
		this.realDelete = realDelete;
	}


	
	
	
}
