package com.sxt.manager.model;

public class Shouru {
	private int id;
	private double jine;  	  				//金额
	private String shouruTime;  			//收入日期
	private String descrpt;   				//描述
	private int userId;						//用户的id
	private int studentId;					//学员的id
	private String shenheState="未审核";  	//未审核，审核通过，审核不通过
	private int shenheId;       			//审核人的id
	
	public Shouru() {
		super();
	}
	public Shouru(int id, double jine, String shouruTime, String descrpt,
			int userId, int studentId, String shenheState, int shenheId) {
		super();
		this.id = id;
		this.jine = jine;
		this.shouruTime = shouruTime;
		this.descrpt = descrpt;
		this.userId = userId;
		this.studentId = studentId;
		this.shenheState = shenheState;
		this.shenheId = shenheId;
	}
	public String getShenheState() {
		return shenheState;
	}
	public void setShenheState(String shenheState) {
		this.shenheState = shenheState;
	}
	public int getShenheId() {
		return shenheId;
	}
	public void setShenheId(int shenheId) {
		this.shenheId = shenheId;
	}
	public int getStudentId() {
		return studentId;
	}
	public void setStudentId(int studentId) {
		this.studentId = studentId;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public double getJine() {
		return jine;
	}
	public void setJine(double jine) {
		this.jine = jine;
	}
	public String getShouruTime() {
		return shouruTime;
	}
	public void setShouruTime(String shouruTime) {
		this.shouruTime = shouruTime;
	}
	public String getDescrpt() {
		return descrpt;
	}
	public void setDescrpt(String descrpt) {
		this.descrpt = descrpt;
	}
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}

}
