package com.sxt.manager.model;

import java.util.Date;

/**
 * 对员工工作进行评价
 * @author ThinkPad
 *
 */
public class PingjiaEmp {
	private int id;
	private String content;
	private Date pingjiaTime; //评价的月份
	private int employeeId;  //被评价人的id
	private int mgrId;   //评价人的id
	private int grade;   //评价等级
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public Date getPingjiaTime() {
		return pingjiaTime;
	}
	public void setPingjiaTime(Date pingjiaTime) {
		this.pingjiaTime = pingjiaTime;
	}
	public int getEmployeeId() {
		return employeeId;
	}
	public void setEmployeeId(int employeeId) {
		this.employeeId = employeeId;
	}
	public int getMgrId() {
		return mgrId;
	}
	public void setMgrId(int mgrId) {
		this.mgrId = mgrId;
	}
	public int getGrade() {
		return grade;
	}
	public void setGrade(int grade) {
		this.grade = grade;
	}
	
	
}
