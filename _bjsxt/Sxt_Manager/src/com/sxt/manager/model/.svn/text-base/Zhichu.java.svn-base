package com.sxt.manager.model;

public class Zhichu {
	
	private int id;
	private double jine; 				//金额
	private String type="办公耗材"; 		//办公耗材，电费，水费，退住宿费,退电脑押金，退学费，学校返点，出差费用，工资，固定资产，通讯费用，网络宣传，校园宣传，交通费用，请客聚会，学员推荐费用，其他
	private String descrpt;				//简要描述
	private int    studentId;			//学员id
	private int    userId;  			//录入者的id
	private int    baoxiaoId;  			//报销人的id
	private String zhichuTime;			//支出日期
	private String sn;  				//票据编号
	private String shenheState="未审核"; //未审核，审核通过，审核不通过
	private int    shenheId;  			//审核人的id
	private String descript ;			//相关描述
	private String approveDescript ;	//审核信息的描述 
	
	public Zhichu() {
		super();
	}
	public Zhichu(int id, double jine, String type, String descrpt,
			int studentId, int userId, int baoxiaoId, String zhichuTime,
			String sn, String shenheState, int shenheId, String descript,
			String approveDescript) {
		super();
		this.id = id;
		this.jine = jine;
		this.type = type;
		this.descrpt = descrpt;
		this.studentId = studentId;
		this.userId = userId;
		this.baoxiaoId = baoxiaoId;
		this.zhichuTime = zhichuTime;
		this.sn = sn;
		this.shenheState = shenheState;
		this.shenheId = shenheId;
		this.descript = descript;
		this.approveDescript = approveDescript;
	}

	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public double getJine() {
		return jine;
	}
	public void setJine(double jine) {
		this.jine = jine;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getDescrpt() {
		return descrpt;
	}
	public void setDescrpt(String descrpt) {
		this.descrpt = descrpt;
	}
	public int getStudentId() {
		return studentId;
	}
	public void setStudentId(int studentId) {
		this.studentId = studentId;
	}
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	public int getBaoxiaoId() {
		return baoxiaoId;
	}
	public void setBaoxiaoId(int baoxiaoId) {
		this.baoxiaoId = baoxiaoId;
	}
	public String getZhichuTime() {
		return zhichuTime;
	}
	public void setZhichuTime(String zhichuTime) {
		this.zhichuTime = zhichuTime;
	}
	public String getSn() {
		return sn;
	}
	public void setSn(String sn) {
		this.sn = sn;
	}
	public String getShenheState() {
		return shenheState;
	}
	public void setShenheState(String shenheState) {
		this.shenheState = shenheState;
	}
	public int getShenheId() {
		return shenheId;
	}
	public void setShenheId(int shenheId) {
		this.shenheId = shenheId;
	}
	public String getDescript() {
		return descript;
	}
	public void setDescript(String descript) {
		this.descript = descript;
	}
	public String getApproveDescript() {
		return approveDescript;
	}
	public void setApproveDescript(String approveDescript) {
		this.approveDescript = approveDescript;
	}
}
